{
    "final_space": true,
    "console_title": true,
    "console_title_style": "template",
    "console_title_template": "{{if .Root}}root :: {{end}}{{.Folder}}",
    "blocks": [
        {
            "type": "prompt",
            "newline": false,
            "alignment": "left",
            "segments": [
                {
                    "type": "os",
                    "style": "diamond",
                    "leading_diamond": "",
                    "foreground": "#26C6DA",
                    "background": "#3f3f3f",
                    "properties": {
                        "alpine": "\uf300",
                        "arch": "\uf303",
                        "centos": "\uf304",
                        "debian": "\uf306",
                        "elementary": "\uf309",
                        "fedora": "\uf30a",
                        "gentoo": "\uf30d",
                        "linux": "\ue712",
                        "macos": "\ue711",
                        "manjaro": "\uf312",
                        "mint": "\uf30f",
                        "opensuse": "\uf314",
                        "raspbian": "\uf315",
                        "ubuntu": "\uf31c",
                        "wsl": "\ue712",
                        "wsl_separator": " on ",
                        "windows": "\ue70f"
                    }
                },
                {
                    "type": "path",
                    "style": "diamond",
                    "foreground": "#34b6dc",
                    "background": "#3f3f3f",
                    "properties": {
                        "style": "mixed",
                        "prefix": "<#7a7a7a></> <#f2f200></> ",
                        "mixed_threshold": 10,
                        "enable_hyperlink": false
                    }
                },
                {
                    "type": "az",
                    "style": "diamond",
                    "foreground": "#b1b1e2",
                    "background": "#3f3f3f",
                    "properties": {
                        "prefix": "<#7a7a7a></>  ",
                        "display_account": false,
                        "display_id": false,
                        "display_name": true,
                        "info_separator": " @ ",
                        "include_folders": [
                            "unknown"
                        ]
                    }
                },
                {
                    "type": "dotnet",
                    "style": "diamond",
                    "foreground": "#6CA35E",
                    "background": "#3f3f3f",
                    "leading_diamond": "",
                    "trailing_diamond": "",
                    "properties": {
                        "prefix": "<#7a7a7a></> <b></b> ",
                        "display_version": true
                    }
                },
                {
                    "type": "terraform",
                    "style": "diamond",
                    "foreground": "#ebcc34",
                    "background": "#3f3f3f",
                    "properties": {
                        "prefix": "<#7a7a7a></>  "
                    }
                },
                {
                    "type": "git",
                    "style": "diamond",
                    "foreground": "#12b016",
                    "background": "#3f3f3f",
                    "properties": {
                        "prefix": "<#7a7a7a></> ",
                        "display_status": true,
                        "display_stash_count": true,
                        "display_upstream_icon": true,
                        "status_colors_enabled": true,
                        "color_background": false,
                        "local_changes_color": "#e8e652",
                        "ahead_and_behind_color": "#42dcd7",
                        "behind_color": "#a46630",
                        "ahead_color": "#bd43b3",
                        "working_color": "#787ada",
                        "staging_color": "#00ffff"
                    }
                },
                {
                    "type": "kubectl",
                    "style": "diamond",
                    "foreground": "#ebcc34",
                    "background": "#3f3f3f",
                    "properties": {
                        "prefix": "<#7a7a7a></> \uFD31 ",
                        "template": "{{.Context}} :: {{if .Namespace}}{{.Namespace}}{{else}}default{{end}}",
                        "include_folders": [
                            "unknown"
                        ]
                    }
                },
                {
                    "type": "text",
                    "style": "diamond",
                    "trailing_diamond": "",
                    "foreground": "#3f3f3f",
                    "background": "#3f3f3f",
                    "properties": {
                        "text": " ",
                        "postfix": "",
                        "prefix": ""
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "text",
                    "style": "diamond",
                    "leading_diamond": "",
                    "foreground": "#3f3f3f",
                    "background": "#3f3f3f",
                    "properties": {
                        "text": " ",
                        "prefix": "",
                        "postfix": " "
                    }
                },
                {
                    "type": "executiontime",
                    "style": "diamond",
                    "foreground": "#83769c",
                    "background": "#3f3f3f",
                    "properties": {
                        "prefix": "羽",
                        "postfix": "\u2800<#7a7a7a></> ",
                        "always_enabled": false,
                        "threshold": 100,
                        "style": "roundrock"
                    }
                },
                {
                    "type": "root",
                    "style": "diamond",
                    "foreground": "#ff8080",
                    "background": "#3f3f3f",
                    "properties": {
                        "prefix": "",
                        "postfix": " <#7a7a7a></> ",
                        "root_icon": ""
                    }
                },
                {
                    "type": "time",
                    "style": "diamond",
                    "foreground": "#689f38",
                    "background": "#3f3f3f",
                    "properties": {
                        "prefix": "<#007ACC>\uf64f</> ",
                        "postfix": "",
                        "time_format": "15:04:05"
                    }
                },
                {
                    "type": "text",
                    "style": "diamond",
                    "trailing_diamond": "",
                    "foreground": "#3f3f3f",
                    "background": "#3f3f3f",
                    "properties": {
                        "text": "",
                        "postfix": "",
                        "prefix": ""
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "newline": true,
            "alignment": "left",
            "segments": [
                {
                    "type": "exit",
                    "style": "plain",
                    "foreground": "#ffffff",
                    "properties": {
                        "display_exit_code": false,
                        "always_enabled": false,
                        "error_color": "#f1184c",
                        "prefix": ""
                    }
                },
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#269024",
                    "properties": {
                        "prefix": "",
                        "text": ""
                    }
                }
            ]
        }
    ]
}